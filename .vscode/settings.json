  "python.analysis.enablePytestExtra": true,
  "python.analysis.enablePytestSupport": true,
  "python.analysis.enableSyncServer": false,
  "python.analysis.exclude": [],
  "python.analysis.extraCommitChars": true,
  "python.analysis.extraPaths": [],
  "python.analysis.fixAll": [],
  "python.analysis.gotoDefinitionInStringLiteral": true,
  "python.analysis.ignore": [],
  "python.analysis.importFormat": "absolute",
  "python.analysis.include": [],
  "python.analysis.indexing": true,
  "python.analysis.inlayHints.callArgumentNames": "off",
  "python.analysis.inlayHints.functionReturnTypes": false,
  "python.analysis.inlayHints.pytestParameters": false,
  "python.analysis.inlayHints.variableTypes": false,
  "python.analysis.logLevel": "Information",
  "python.analysis.persistAllIndices": true,
  "python.analysis.stubPath": "typings",
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.typeshedPaths": [],
  "python.analysis.useLibraryCodeForTypes": true,
  "python.analysis.userFileIndexingLimit": 2000,
  "python.analysis.packageIndexDepths": [
    {
      "name": "sklearn",
      "depth": 2
    },
    {
      "name": "matplotlib",
      "depth": 2
    },
    {
      "name": "scipy",
      "depth": 2
    },
    {
      "name": "django",
      "depth": 2
    },
    {
      "name": "flask",
      "depth": 2
    },
    {
      "name": "fastapi",
      "depth": 2
    }
  ],

  "python.autoComplete.extraPaths": [],
  "python.envFile": "${workspaceFolder}/.env",
  "python.globalModuleInstallation": false,
  "python.languageServer": "Default",
  "python.missingPackage.severity": "Hint",
  "python.REPL.enableREPLSmartSend": true,
  "python.terminal.activateEnvInCurrentTerminal": false,
  "python.terminal.activateEnvironment": true,
  "python.terminal.executeInFileDir": false,
  "python.terminal.focusAfterLaunch": false,
  "python.terminal.launchArgs": [],
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.testing.cwd": "",
  "python.testing.debugPort": 3000,
  "python.testing.promptToConfigure": true,
  "python.testing.pytestArgs": [],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestArgs": ["-v", "-s", ".", "-p", "*test*.py"],
  "python.testing.unittestEnabled": false,

  // Ruff
  "ruff.enable": true,
  "ruff.organizeImports": true,
  "ruff.fixAll": true,
  "ruff.format.args": [],
  "ruff.lint.args": [],
  "ruff.lint.run": "onType",
  "ruff.importStrategy": "fromEnvironment",
  "ruff.codeAction.disableRuleComment": {
    "enable": true
  },
  "ruff.codeAction.fixViolation": {
    "enable": true
  },
  "ruff.interpreter": [],
  "ruff.path": [],
  "ruff.showNotifications": "off",
  "ruff.trace.server": "off",

  // Qt for Python
  "qtForPython.qmlls.enabled": true,
  "qtForPython.qmlls.options": [],
  "qtForPython.qmlls.path": "",

  "qtForPython.rcc.liveExecution.enabled": true,
  "qtForPython.rcc.liveExecution.glob": "**/*.qrc",
  "qtForPython.rcc.options": [
    "-o",
    "${resourceDirname}${pathSeparator}src${pathSeparator}${resourceBasenameNoExtension}_rc.py"
  ],
  "qtForPython.rcc.path": "",

  "qtForPython.qml.options": [],
  "qtForPython.qml.path": "",

  "qtForPython.qmlformat.options": [],
  "qtForPython.qmlformat.path": "",

  "[json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true
  },
  "cSpell.words": [
    "appsettings",
    "assertpy",
    "httpx",
    "levelname",
    "MVVM",
    "picologging",
    "pydantic",
    "qtpy",
    "richillcapital"
  ]
